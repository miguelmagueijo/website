---
    import LinkA from "./LinkA.astro";

    interface Props {
        className?: string
    }

    const { className } = Astro.props as Props;
---
<ul class={ className } class="underline md:text-lg">
    <LinkA href="/" text="Home" />
    <LinkA href="/contact" text="Contact" />
    <LinkA href="/forged-in-space" text="ForgedIn.Space" />
</ul>